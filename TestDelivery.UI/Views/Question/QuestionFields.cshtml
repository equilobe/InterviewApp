@model TestDelivery.DAL.Questions.Question
@Html.HiddenFor(model => model.Id)
@Html.HiddenFor(model => model.Tags)

@Html.EditableFieldFor(m => m.Summary)
@Html.EditableFieldFor(m => m.Content, raw: true)

@Html.EditableFieldFor(m => m.TimeLimit)
@Html.EditableFieldFor(m => m.Score)


<script type="text/javascript">
    $(function () {

        if (typeof (canEditTemplate) === "boolean" && !canEditTemplate) {
            $("input[name='" + "@Html.NameFor(m => m.Score)" + "']").attr("disabled", "");
        }

    });

</script>
